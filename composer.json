{
    "name": "meduo/igts",
    "description": "Meduo.net is a leading online healthcare & medical education platform in the MENA region",
    "keywords": [
        "meduo"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "chumper/zipper": "1.0.x",
        "consoletvs/links": "^1.0",
        "darryldecode/cart": "dev-master",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "firebase/php-jwt": "^5.5",
        "fruitcake/laravel-cors": "2.0.1",
        "intervention/image": "^2.5",
        "kreait/laravel-firebase": "^1.0@dev",
        "laravel/framework": "5.8.*",
        "laravel/passport": "7.5.1",
        "laravel/socialite": "^3.2.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.8.0",
        "mcamara/laravel-localization": "^1.3",
        "mpociot/laravel-apidoc-generator": "^4.8.2",
        "themsaid/laravel-langman": "dev-master",
        "torann/geoip": "^1.0@dev",
        "uxweb/sweet-alert": "^1.4",
        "vimeo/vimeo-api": "^2.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "composer/composer": "^2.0@dev",
        "filp/whoops": "~2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Application/Helpers/arrays.php",
            "app/Application/Helpers/model.php",
            "app/Application/Helpers/uploadFiles.php",
            "app/Application/Helpers/setting.php",
            "app/Application/Helpers/menu.php",
            "app/Application/Helpers/user.php",
            "app/Application/Helpers/lang.php",
            "app/Application/Helpers/adminTheme.php",
            "app/Application/Helpers/api.php",
            "app/Application/Helpers/website.php",
            "app/Application/Helpers/controllers.php",
            "app/Application/Helpers/widget.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3"
        },
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
