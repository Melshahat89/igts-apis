{
    "name": "meduo/igts",
    "description": "Meduo.net is a leading online healthcare & medical education platform in the MENA region",
    "keywords": [
        "meduo"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "aporat/store-receipt-validator": "^1.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "^2.13.9",
        "facade/ignition": "^2.17.7",
        "fideloper/proxy": "^4.4.2",
        "firebase/php-jwt": "^6.10.0",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.9",
        "imdhemy/laravel-purchases": "1.x-dev",
        "inertiajs/inertia-laravel": "^0.5",
        "intervention/image": "^2.7",
        "jantinnerezo/livewire-range-slider": "^1.0",
        "kreait/laravel-firebase": "^4.2.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/jetstream": "^2.9.0",
        "laravel/legacy-factories": "^1.0",
        "laravel/passport": "^10.0",
        "laravel/sanctum": "^2.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "mcamara/laravel-localization": "^1.7.0",
        "nunomaduro/collision": "^5.0",
        "torann/geoip": "^3.0",
        "vimeo/vimeo-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Application/Helpers/arrays.php",
            "app/Application/Helpers/model.php",
            "app/Application/Helpers/uploadFiles.php",
            "app/Application/Helpers/setting.php",
            "app/Application/Helpers/menu.php",
            "app/Application/Helpers/user.php",
            "app/Application/Helpers/lang.php",
            "app/Application/Helpers/adminTheme.php",
            "app/Application/Helpers/api.php",
            "app/Application/Helpers/website.php",
            "app/Application/Helpers/controllers.php",
            "app/Application/Helpers/widget.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
