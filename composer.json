{
    "name": "meduo/igts",
    "description": "Meduo.net is a leading online healthcare & medical education platform in the MENA region",
    "keywords": [
        "meduo"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3||^8.0",
        "aporat/store-receipt-validator": "dev-master",
        "barryvdh/laravel-dompdf": "dev-master",
        "doctrine/dbal": "2.13.9",
        "facade/ignition": "2.17.7",
        "fideloper/proxy": "4.4.2",
        "firebase/php-jwt": "v6.10.0",
        "fruitcake/laravel-cors": "2.2.x-dev",
        "guzzlehttp/guzzle": "7.9.x-dev",
        "inertiajs/inertia-laravel": "dev-master",
        "intervention/image": "2.7.x-dev",
        "jantinnerezo/livewire-range-slider": "dev-main",
        "kreait/laravel-firebase": "4.2.0",
        "laravel/framework": "8.x-dev",
        "laravel/horizon": "dev-master",
        "laravel/jetstream": "v2.9.0",
        "laravel/legacy-factories": "dev-master",
        "laravel/passport": "10.x-dev",
        "laravel/sanctum": "2.x-dev",
        "laravel/socialite": "5.x-dev",
        "laravel/telescope": "dev-master",
        "laravel/tinker": "2.x-dev",
        "laravel/ui": "3.x-dev",
        "mcamara/laravel-localization": "^1.7.0",
        "nunomaduro/collision": "^5.0",
        "torann/geoip": "^3.0.2",
        "vimeo/vimeo-api": "^2.0.5"
    },
    "require-dev": {
        "composer/composer": "^2.0@dev",
        "filp/whoops": "~2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Application/Helpers/arrays.php",
            "app/Application/Helpers/model.php",
            "app/Application/Helpers/uploadFiles.php",
            "app/Application/Helpers/setting.php",
            "app/Application/Helpers/menu.php",
            "app/Application/Helpers/user.php",
            "app/Application/Helpers/lang.php",
            "app/Application/Helpers/adminTheme.php",
            "app/Application/Helpers/api.php",
            "app/Application/Helpers/website.php",
            "app/Application/Helpers/controllers.php",
            "app/Application/Helpers/widget.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
